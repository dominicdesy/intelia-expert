            }
        
        # Construction message final
        lang = language if language in intro_message else "fr"
        response_text = intro_message[lang] + "\n\n"
        
        for i, question_text in enumerate(questions, 1):
            response_text += f"{i}. {question_text}\n"
        
        response_text += "\n"
        
        # Exemples sp√©cialis√©s
        if poultry_type == "layers":
            examples = {
                "fr": "**Exemple complet :** 'Mes pondeuses ISA Brown de 25 semaines produisent seulement 12 ≈ìufs par jour au lieu de 18. Elles sont log√©es en cages et re√ßoivent 16h de lumi√®re.'",
                "en": "**Complete example:** 'My 25-week ISA Brown layers are producing only 12 eggs per day instead of 18. They are housed in cages and receive 16h of light.'",
                "es": "**Ejemplo completo:** 'Mis ponedoras ISA Brown de 25 semanas producen solo 12 huevos por d√≠a en lugar de 18. Est√°n alojadas en jaulas y reciben 16h de luz.'"
            }
        elif poultry_type == "broilers":
            examples = {
                "fr": "**Exemple complet :** 'Mes poulets Ross 308 m√¢les de 21 jours p√®sent 650g. Est-ce normal ?'",
                "en": "**Complete example:** 'My 21-day Ross 308 male broilers weigh 650g. Is this normal?'",
                "es": "**Ejemplo completo:** 'Mis pollos Ross 308 machos de 21 d√≠as pesan 650g. ¬øEs normal?'"
            }
        else:
            examples = {
                "fr": "**Exemple :** 'Ross 308 m√¢les' ou 'Pondeuses ISA Brown'",
                "en": "**Example:** 'Ross 308 males' or 'ISA Brown layers'",
                "es": "**Ejemplo:** 'Ross 308 machos' o 'Ponedoras ISA Brown'"
